<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         version="2.5"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>PostServlet</servlet-name>
    <servlet-class>ru.job4j.dream.servlet.PostServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PostEditServlet</servlet-name>
    <servlet-class>ru.job4j.dream.servlet.PostEditServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CandidateServlet</servlet-name>
    <servlet-class>ru.job4j.dream.servlet.CandidateServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CandidateEditServlet</servlet-name>
    <servlet-class>ru.job4j.dream.servlet.CandidateEditServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>IndexServlet</servlet-name>
    <servlet-class>ru.job4j.dream.servlet.IndexServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UploadServlet</servlet-name>
    <servlet-class>ru.job4j.dream.servlet.UploadServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DownloadServlet</servlet-name>
    <servlet-class>ru.job4j.dream.servlet.DownloadServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UploadCandidatePhoto</servlet-name>
    <servlet-class>ru.job4j.dream.servlet.UploadCandidatePhoto</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ViewPhotoServlet</servlet-name>
    <servlet-class>ru.job4j.dream.servlet.ViewPhotoServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CandidateDeleteServlet</servlet-name>
    <servlet-class>ru.job4j.dream.servlet.CandidateDeleteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CandidateDeleteServlet</servlet-name>
    <url-pattern>/candidate/delete.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ViewPhotoServlet</servlet-name>
    <url-pattern>/candidate/viewPhotoServlet.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UploadCandidatePhoto</servlet-name>
    <url-pattern>/candidate/uploadCandidatePhoto.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UploadServlet</servlet-name>
    <url-pattern>/upload.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DownloadServlet</servlet-name>
    <url-pattern>/download.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PostServlet</servlet-name>
    <url-pattern>/posts.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PostEditServlet</servlet-name>
    <url-pattern>/post/edit.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CandidateServlet</servlet-name>
    <url-pattern>/candidates.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CandidateEditServlet</servlet-name>
    <url-pattern>/candidate/edit.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>IndexServlet</servlet-name>
    <url-pattern>/index.do</url-pattern>
  </servlet-mapping>
</web-app>